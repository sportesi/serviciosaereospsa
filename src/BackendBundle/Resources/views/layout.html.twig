<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

        <title>Servicios A&eacute;reos PSA</title>

        <link rel="stylesheet" href="{{ asset("bundles/backend/bower_components/bootstrap/dist/css/bootstrap.min.css") }}" type="text/css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
        <link rel="stylesheet" href="{{ asset("/bundles/backend/css/AdminLTE.min.css") }}">
        <link rel="stylesheet" href="{{ asset("/bundles/backend/css/skins/skin-blue.min.css") }}">
        <link rel="stylesheet" href="{{ asset("bundles/backend/bower_components/bootstrap-sweetalert/dist/sweetalert.css") }}" type="text/css">
        <link rel="stylesheet" href="{{ asset("bundles/backend/css/styles.css") }}" type="text/css">

        {% block styles %}{% endblock %}
    </head>
    <body class="hold-transition skin-blue sidebar-mini">
        <div class="wrapper">

            {% include('BackendBundle:CommonViews:header.html.twig') %}

            {% include('BackendBundle:CommonViews:sidebar.new.html.twig') %}

            <div class="content-wrapper">
                {#<section class="content-header">#}
                    {#<h1>#}
                        {#Dashboard#}
                    {#</h1>#}
                {#</section>#}

                <section class="content">
                    {% block body %}

                    {% endblock %}
                </section>
            </div>

        </div>

        <script src="{{ asset("bundles/backend/bower_components/jquery/dist/jquery.min.js") }}"></script>
        <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
        <script>
            $.widget.bridge('uibutton', $.ui.button);
            $('.sidebar-menu')
                .find('a[href="'+window.location.pathname+'"]')
                .parent()
                .addClass('active');
        </script>
        <script src="{{ asset("bundles/backend/bower_components/bootstrap/dist/js/bootstrap.min.js") }}"></script>
        <script src="{{ asset("bundles/backend/bower_components/bootstrap-sweetalert/dist/sweetalert.min.js") }}"></script>
        <script src="{{ asset("/bundles/backend/scripts/app.min.js") }}"></script>
        <script src="{{ asset("bundles/backend/scripts/main.js") }}"></script>

        {% block scripts %}{% endblock %}

    </body>
</html>
